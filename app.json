{
  "expo": {
    "name": "Tride",
    "slug": "Tride",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/tride.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/tridewhite.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tride.png",
        "backgroundColor": "#000000"
      },
      "package": "com.realagara.shoppingapp"
    },
    "web": {
      "favicon": "./assets/tride.png"
    },
    "extra": {
      "eas": {
        "projectId": "96c69de2-ae51-4e60-b1ab-19baa523221d"
      }
    },

    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ]

  }
}
